  "components": {
    "schemas": {
      "AgentLogin": {
        "required": [
          "agentID",
          "password"
        ],
        "type": "object",
        "properties": {
          "agentID": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "AgentProfileUpdate": {
        "required": [
          "agentID",
          "email",
          "mobile",
          "name"
        ],
        "type": "object",
        "properties": {
          "agentID": {
            "minLength": 1,
            "type": "string"
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "mobile": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "currentPassword": {
            "type": "string",
            "nullable": true
          },
          "newPassword": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "AgentSignUp": {
        "required": [
          "email",
          "mobile",
          "name",
          "password"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "agentID": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "mobile": {
            "minLength": 1,
            "pattern": "^[0-9]{10}$",
            "type": "string"
          },
          "password": {
            "minLength": 8,
            "pattern": "^(?=.*[A-Z])(?=.*[a-z])(?=.*\\d)(?=.*[@$!%*?&])[A-Za-z\\d@$!%*?&]{8,}$",
            "type": "string"
          },
          "confirmPassword": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "BotRequest": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ChangePassword": {
        "required": [
          "currentPassword",
          "email",
          "name",
          "newPassword",
          "phone",
          "supervisorID"
        ],
        "type": "object",
        "properties": {
          "supervisorID": {
            "minLength": 1,
            "type": "string"
          },
          "name": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "phone": {
            "minLength": 1,
            "pattern": "^[0-9]{10}$",
            "type": "string"
          },
          "currentPassword": {
            "minLength": 1,
            "type": "string"
          },
          "newPassword": {
            "minLength": 1,
            "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d).{6,}$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "GeminiChatRequest": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LeaveRequest": {
        "required": [
          "agentName"
        ],
        "type": "object",
        "properties": {
          "leaveID": {
            "type": "integer",
            "format": "int32"
          },
          "agentID": {
            "type": "string",
            "nullable": true
          },
          "agentName": {
            "minLength": 1,
            "type": "string"
          },
          "leaveDate": {
            "type": "string",
            "format": "date-time"
          },
